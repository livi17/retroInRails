<h3>Results:</h3>
<table class="table table-striped table-bordered">
  <tbody>
	<% for event in @events %>
	<tr>
		<td><%= event.id %></td>
		<td><%= event.title %></td>
		<td><%= event.address %></td>
		<td><%= event.start_date %></td>
		<td><%= event.end_date %></td>
	</tr>
	<% end %>
</tbody>
</table>


<div id="events"><%= render 'events' %></div>